<% provide(:title, 'RSS Topics') %>
<h1>All rss-Topics</h1>
<%= link_to 'New Topic', new_topic_path %>
<table>
  <tbody>
    <% @topics.each do |topic| %>
        <h3><%= topic.topic %></h3>
        <%= link_to 'Show', topic %> |
        <%= link_to 'Edit', edit_topic_path(topic) %>
    <% end %>
  </tbody>
</table>
