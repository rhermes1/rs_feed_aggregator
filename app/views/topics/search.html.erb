<% unless @topics.blank?%>
  <div class="news">
    <% @topics.each do |result| %>
      <legend><%= link_to result.topic, result %></legend>
        <div class="new_contact">
          <% result.rsses.each do |rss| %>
            <li><%= rss.link %></li>
          <% end %>
        </div>
    <% end %>
  </div>
<% end %>
